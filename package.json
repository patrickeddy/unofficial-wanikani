{
  "name": "unofficial-wanikani",
  "version": "0.0.9",
  "description": "An unofficial JavaScript client for the WaniKani API.",
  "type": "module",
  "main": "./dist/unofficial-wanikani.umd.cjs",
  "module": "./dist/unofficial-wanikani.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/unofficial-wanikani.js",
      "require": "./dist/unofficial-wanikani.umd.cjs"
    }
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "wanikani",
    "api",
    "typescript",
    "client"
  ],
  "author": "Patrick Eddy",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.5.4",
    "vite": "^6.3.3",
    "vite-plugin-dts": "^4.5.3"
  },
  "dependencies": {
    "cross-fetch": "^4.1.0"
  }
}
